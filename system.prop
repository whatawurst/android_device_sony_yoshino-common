### AUDIO
# AAC 5.1 output enabled
media.aac_51_output_enabled=true

# Audioflinger heap size
ro.af.client_heap_size_kbyte=7168

# Deep buffer enable
audio.deep_buffer.media=true

# Fast audio output client buffer size reduction
af.fast_track_multiplier=1

# Offload minimum duration
audio.offload.min.duration.secs=30

# Offload video playback disable
audio.offload.video=false

# QC multimedia parser
vendor.mm.enable.qcom_parser=13342781

# Safe media force enable
audio.safemedia.force=true

# Sony effect min CPU freq
sony.effect.cpu_min_freq_big=600
sony.effect.cpu_min_freq_little=600

# Stagefright deep audio
media.stagefright.audio.deep=true

### BLUETOOTH
bt.max.hfpclient.connections=1
persist.vendor.btstack.a2dp_offload_cap=false

# A4WP profile support
ro.bluetooth.a4wp=false

# Wipower support
ro.bluetooth.emb_wp_mode=true
ro.bluetooth.wipower=true

### CHARGER
ro.charger.disable_init_blank=true
ro.charger.enable_suspend=true

### DISPLAY
# Assertive display
ro.qcom.ad=1
ro.vendor.display.ad=1
ro.vendor.display.ad.sdr_calib_data=/vendor/etc/ad_calib.cfg
ro.vendor.display.sensortype=2

# CABL
ro.qualcomm.cabl=0

# Display default color mode enable
vendor.display.enable_default_color_mode=1

# HWComposer
debug.sf.enable_hwc_vds=1
debug.sf.hw=1
debug.gralloc.enable_fb_ubwc=1
debug.sf.latch_unsignaled=1
vendor.display.perf_hint_window=50

# HWUI
debug.hwui.use_buffer_age=false

# Display default color mode enable
vendor.display.enable_default_color_mode=1

# Gamut
ro.sys.colorgamut.supported=true
persist.sys.colorgamut.mode=1

# HDR
sdm.disable_hdr_lut_gen=1
vendor.vidc.enc.disable.pq=true

# OpenGLES version
ro.opengles.version=196610

# VDS WFD solution
persist.hwc.enable_vds=1

# Xfrm support
persist.service.xrfm.mode=1
ro.service.xrfm.supported=true

### KEYPROVD
# FIDO key provision state and version
persist.keyprovd.fido.prov=false
persist.keyprovd.fido.version=0

### MEDIA PROFILES
media.settings.xml=/vendor/etc/media_profiles_vendor.xml

### NFC
ro.nfc.on.default=false
ro.nfc.port=I2C
ro.nfc.se.sim.enable=true
ro.nfc.se.smx.enable=false

### RADIO
# CNE
persist.vendor.cne.feature=1
persist.vendor.dpm.tcm=1

# OpenCS
ro.semc.version.opencs=1

# Mobile data disable on first boot after clean flash
ro.com.android.prov_mobiledata=false

# Netmgr
persist.data.netmgrd.qos.enable=true
persist.vendor.data.mode=concurrent
ro.vendor.use_data_netmgrd=true

# Rild
ril.subscription.types=NV,RUIM

# Rild - NITZ
persist.rild.nitz_long_ons_0=
persist.rild.nitz_long_ons_1=
persist.rild.nitz_long_ons_2=
persist.rild.nitz_long_ons_3=
persist.rild.nitz_plmn=
persist.rild.nitz_short_ons_0=
persist.rild.nitz_short_ons_1=
persist.rild.nitz_short_ons_2=
persist.rild.nitz_short_ons_3=

# Rmnet data
persist.data.df.agg.dl_pkt=10
persist.data.df.agg.dl_size=4096
persist.data.df.dl_mode=5
persist.data.df.dev_name=rmnet_usb0
persist.data.df.iwlan_mux=9
persist.data.df.mux_count=8
persist.data.df.ul_mode=5
persist.data.wda.enable=true
persist.rmnet.data.enable=true

# Telephony
ro.com.android.dataroaming=false
telephony.lteOnCdmaDevice=0

